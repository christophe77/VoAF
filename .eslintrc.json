{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "plugins": ["sonarjs"],
  "extends": ["airbnb-base","plugin:sonarjs/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "implicit-arrow-linebreak": 0,
    "function-paren-newline": 0,
    "comma-dangle": 0
  }
}
